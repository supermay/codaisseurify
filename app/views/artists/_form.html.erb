<%= form_for @artist, html: { multipart: true } do |f| %>
<div class="form-group">
  <div class="form-group">
    <label>Name</label>
    <%= f.text_field :name, placeholder: "Name" %>
    <br/>
  </div>

  <div class="form-group">
    <label>Image Upload</label>
    <%= file_field_tag "image", type: :file %>
    <br />
  </div>

  <div class="form-group">
    <label>Country</label>
    <%= f.text_field :country, placeholder: "Country" %>
    <br />
  </div>

  <div class="form-group">
    <label>Band?</label>
    <%= f.check_box :is_band %>
    <br />
  </div>
  <div class="form-group">
    <label>Website</label>
    <%= f.text_field :website, placeholder: "Website" %>
    <br />
  </div>

  <div class="actions">
  <%= f.submit "Create" %>
  </div>

</div>
<% end %>
