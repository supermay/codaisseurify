<%= form_for @artist, html: { multipart: true } do |f| %>
  <label>Name</label>
  <%= f.text_field :name, placeholder: "Name" %>
  <br/>
  <label>Image</label>
  <%= file_field_tag "image", type: :file, placeholder: "Upload and image" %>
  <br />
  <label>Country</label>
  <%= f.text_field :country, placeholder: "Country" %>
  <br />
  <label>Band?</label>
  <%= f.check_box :is_band %>
  <br />
  <label>Website</label>
  <%= f.text_field :website, placeholder: "Website" %>
  <br />
  <%= f.submit "Create" %>
<% end %>
